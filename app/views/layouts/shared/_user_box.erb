<ul class="users nav nav-list">
  <% sidebar_users.each do |user| %>
      <li>
        <%= avatar(user, :list) %>
        <%= link_to user_path(user) do %>
            <i class="icon icon-asterisk"
               style="color: <%= user.login_status(current_user.name) %>"></i>
            <%= user.name %>
        <% end %>
      </li>
  <% end %>
</ul>