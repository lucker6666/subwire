= render layout: 'frame', locals: {title: t('wiki.headline')} do
  %ul
    - @pages.each do |page|
      %li
        = link_to channel_wiki_path(@current_channel, page), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-mini pull-right' do
          %i.icon.icon-trash
        = link_to page.title, channel_wiki_path(@current_channel, page)